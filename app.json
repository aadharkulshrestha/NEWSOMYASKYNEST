{
  "expo": {
    "name": "Somya Sky Nest",
    "slug": "SOMYASKYNEST",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/apartment.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/apartment.png",
